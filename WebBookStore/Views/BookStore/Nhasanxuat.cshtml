@model IEnumerable<WebBookStore.Models.NHAXUATBAN>
<p class=" lead">Nhà xuất bản</p>
<div class="list-group">
    @foreach (var item in Model)
    {
        <a href="@Url.Action("SPtheoNhaXuatBan" , "BookStore" , new {id = item.MaNXB })" class="list-group-item">
            @item.TenNXB
        </a>
    }
</div>
